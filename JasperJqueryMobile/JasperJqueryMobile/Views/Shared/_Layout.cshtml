<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>


    <!-- Jquery-->
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
</head>
<body>
    <div class="navbar-fixed-top">
        <div class="container">
            <div data-role="header" data-theme="b">
                <div data-role="navbar">
                    <ul>
                        <li><a href="#anylink">Home</a></li>
                        <li><a href="#anylink">Page Two</a></li>
                        <li><a href="#anylink">Search</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="ui-body ui-body-b"  data-theme="b" data-content-theme="b">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Jasper Jquery Mobile Application</p>
        </footer>
    </div>


    <!-- Jquery Mobile-->
    <script src="~/Content/jquery.mobile-1.4.2.js" type="text/javascript"></script>
    <link href="~/Content/jquery.mobile-1.4.2.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery.mobile.theme-1.4.2.css" rel="stylesheet" type="text/css" />

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
